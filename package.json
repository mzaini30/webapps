    "@unocss/transformer-directives": "^0.39.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vueuse/head": "^0.7.6",
    "bumpp": "^7.2.0",
    "critters": "^0.0.16",
    "prettier": "^2.7.0",
    "typescript": "^4.5.4",
    "unocss": "^0.39.0",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.7",
    "vite-plugin-pages": "^0.24.1",
    "vite-plugin-vue-inspector": "github:mzaini30/vite-plugin-vue-inspector",
    "vite-plugin-yaml2": "^0.2.4",
    "vite-ssg": "^0.20.1",
    "vue-lazyload": "3.0.0-rc.2",
    "vue-router": "^4.0.16",
    "vue-router-prefetch": "^1.6.3",
    "vue-tsc": "^0.34.7"
