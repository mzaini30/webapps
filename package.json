{
  "name": "webapps",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview",
    "prettier": "prettier --write 'src/**/*.{vue,ts,css}' vite.config.ts index.html",
